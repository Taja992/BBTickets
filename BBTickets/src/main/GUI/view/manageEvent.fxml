<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="330.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.controller.ManageEventController">
   <children>
      <TextField fx:id="eventTypeField" layoutX="77.0" layoutY="121.0" promptText="Event Type" />
      <TextField fx:id="eventLocationField" layoutX="77.0" layoutY="155.0" promptText="Event Location" />
      <DatePicker fx:id="eventStartDatePicker" layoutX="77.0" layoutY="192.0" promptText="Start Time" />
      <Spinner fx:id="eventHourSpinner" initialValue="12" layoutX="90.0" layoutY="228.0" max="23" min="0" prefHeight="25.0" prefWidth="54.0" />
      <Spinner fx:id="eventMinuteSpinner" initialValue="30" layoutX="161.0" layoutY="228.0" max="59" min="0" prefHeight="25.0" prefWidth="54.0" />
      <Label layoutX="150.0" layoutY="232.0" text=":" />
      <Button layoutX="77.0" layoutY="288.0" mnemonicParsing="false" onAction="#manageEvent" styleClass="addEvent" text="Edit Event" />
      <TextField fx:id="eventIdField" editable="false" layoutX="78.0" layoutY="87.0" promptText="Event ID (cannot edit)" />
   </children>
</AnchorPane>
